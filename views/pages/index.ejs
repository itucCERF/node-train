<%- contentFor('title') -%>
EJS Is Fun
<%- contentFor('addCss') -%>
<%- contentFor('addJs') -%>
<%- contentFor('body') -%>
<div class="jumbotron">
    <h1>This is great</h1>
    <p>Welcome to templating using EJS</p>
    <h2>Variable</h2>
    <p><%= tagline %></p>
    <ul>
        <% mascots.forEach(function(mascot) { %>
            <li>
                <strong><%= mascot.name %></strong>
                representing <%= mascot.organization %>,
                born <%= mascot.birth_year %>
            </li>
        <% }); %>
    </ul>
</div>

